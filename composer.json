{
    "name": "zalas/param-converter-bundle",
    "description": "Bundle that provides additional param converters for Symfony",
    "type": "bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Zalas",
            "email": "jakub@zalas.pl"
        }
    ],
    "require": {
        "php": "^5.5||^7.0",
        "sensio/framework-extra-bundle": "~3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.5",
        "phpunit/phpunit": "~4.6",
        "symfony/browser-kit": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Zalas\\Bundle\\ParamConverterBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppBundle\\": "Tests/Integration/app/src/AppBundle"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
